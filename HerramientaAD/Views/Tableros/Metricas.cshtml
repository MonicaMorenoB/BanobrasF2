                                                    <td>
                                                        @if (int.Parse(proc.Attributes["Obsolescencia"].Value.ToString()) == 1)
                                                        {
                                                            <p> Alto</p>
                                                        }
                                                        @if (int.Parse(proc.Attributes["Obsolescencia"].Value.ToString()) == 2)
