                                                        }
                                                        @if (int.Parse(proc.Attributes["Obsolescencia"].Value.ToString()) == 2)
                                                        {
                                                            <img width="25" height="25" src="~/Images/c_amarillo.png" />
                                                        }
                                                        @if (int.Parse(proc.Attributes["Obsolescencia"].Value.ToString()) == 3)
                                                        {
                                                            <img width="25" height="25" src="~/Images/e_rojo.png" />
                                                        }
