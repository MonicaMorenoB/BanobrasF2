                                                        @if (int.Parse(proc.Attributes["Ser_Inter"].Value.ToString()) >= 4 && int.Parse(proc.Attributes["Ser_Inter"].Value.ToString()) <= 5)
                                                        {
                                                            <p>@proc.Attributes["Ser_Inter"].Value.ToString() Encontrados</p>
