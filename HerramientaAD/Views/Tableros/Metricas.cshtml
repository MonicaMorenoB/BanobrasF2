                                                        @if (int.Parse(proc.Attributes["Ser_Inter"].Value.ToString()) >= 6 && int.Parse(proc.Attributes["Ser_Inter"].Value.ToString()) <= 8)
                                                        {
                                                            <p>@proc.Attributes["Ser_Inter"].Value.ToString() Encontrados</p>
                                                        }
                                                        @if (int.Parse(proc.Attributes["Ser_Inter"].Value.ToString()) > 8)
                                                        {
                                                            <p>@proc.Attributes["Ser_Inter"].Value.ToString() Encontrados</p>
                                                        }
                                                    </td>

                                                </tr>
                                            </table>
                                        </td>
                                    }
                                }
